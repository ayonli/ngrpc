{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2018",
        "newLine": "LF",
        "importHelpers": true,
        "strictNullChecks": true,
        "noUnusedParameters": true,
        "noUnusedLocals": true,
        "noImplicitThis": true,
        "sourceMap": true,
        "declaration": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "paths": {
            "@hyurl/grpc-boot": [
                "./index"
            ]
        }
    },
    "include": [
        "*.ts",
        "services/*.ts"
    ]
}
