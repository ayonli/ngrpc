{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2018",
        "newLine": "LF",
        "importHelpers": true,
        "experimentalDecorators": true,
        "noUnusedParameters": true,
        "noUnusedLocals": true,
        "noImplicitThis": true,
        "sourceMap": true,
        "declaration": true,
        "resolveJsonModule": true,
        "paths": {
            "@hyurl/grpc-async": [
                "../grpc-async"
            ]
        }
    },
    "include": [
        "*.ts",
        "**/*.ts"
    ]
}
