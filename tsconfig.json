{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2018",
        "outDir": "./dist",
        "incremental": true,
        "newLine": "LF",
        "importHelpers": true,
        "strictNullChecks": true,
        "noUnusedParameters": true,
        "noUnusedLocals": true,
        "noImplicitThis": true,
        "sourceMap": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "paths": {
            "@ayonli/ngrpc": [
                "."
            ]
        }
    },
    "include": [
        "*.ts",
        "*/**.ts"
    ]
}
